{
  "expo": {
    "name": "network",
    "slug": "network",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Yeet to access your camera."
        }
      ]
    ],
    "platforms": ["ios"], 
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/yeet.png",
      "backgroundColor": "#f2ecde",
      "resizeMode": "auto"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSAllowsArbitraryLoads": true
      }
    },
    "android": {
      "adaptiveIcon": {}
    },
    "web": {}
  }
}